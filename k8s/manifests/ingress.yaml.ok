apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: go-web-app
  annotations:
    # ‚ùå REMOVE THIS LINE:
    # nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  ingressClassName: nginx
  rules:
    - http:
        paths:
          - path: /courses
            pathType: Prefix
            backend:
              service:
                name: go-web-app
                port:
                  number: 80
